	"library": "./library.js",
	"hooks": [
		{ "hook": "static:app.load", "method": "init" },
		{ "hook": "filter:admin.header.build", "method": "addAdminNavigation" },
		{ "hook": "action:settings.set", "method": "onSettingsChange" },
		{ "hook": "filter:privileges.groups.list_human", "method": "addPrivileges" },
